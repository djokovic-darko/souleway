      {% assign search_products = search.results | where: "object_type", "product" %}
      {% for product in search_products %}
        {% assign sku-list = sku-list | append: product.variants.first.sku | append: ';' %}
      {% endfor %}
      {% assign search_product_ids_string = sku-list %}

      <script type="text/javascript" src="//www.adcell.de/js/inlineretarget.js?method=search&pid=undefined&search={{search.terms}}&productIds={{search_product_ids_string}}&productSeparator=;" async="async"></script>
