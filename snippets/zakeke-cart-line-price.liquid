{% assign customization_price = item.properties._configurationPrice | default: 0 | times: item.quantity %}
{% if customization_price > 0 %}
	{{ item.line_price | plus: customization_price | money }}
{% else %}
	<div class="un-line-item-total" data-id="{{ item.key }}">{{ item.line_price | money }}</div>
{% endif %}