@media (max-width:768px) {
    .slick-slide{
      margin-right:15px;
    }
        
   .arrow-left{
      background: #000;
      height: 40px;
      min-height: 40px;
      width: 40px;
      min-width:40px;
      padding: 10px;
      border-radius: 50%;
      color: #fff;
      font-weight: bold;
      position: absolute;
      opacity:0.8;
      left: 10px;
      bottom: 45%;
      z-index: 1;
      font-size:14px;

      &:focus {
      outline:none;
      }
    }

  .arrow-left.slick-disabled{
     display:none !important;
  }

  .arrow-right{
      background: #000;
      height: 40px;
      min-height: 40px;
      width: 40px;
      min-width:40px;
      padding: 10px;
      opacity:0.8;
      border-radius: 50%;
      color: #fff;
      font-weight: bold;
      position: absolute;
      right: 10px;
      bottom: 45%;
      z-index: 1;
      font-size:14px;

    &:focus {
      outline:none;
    }
  }

  .arrow-right.slick-disabled{
     display:none !important;
  }

}

@media (max-width:799px) {

   .arrow-left-customiser{
      padding: 0 10px;
      top: 0;
      height: 100%;
      color: #000;
      position: absolute;
      left: 0;
      z-index: 1;

      &:focus {
      outline:none;
      }
       [class^="icon-"]:before,
       [class*=" icon-"]:before {
         font-weight: 700;
         opacity: 0.5;
         transform: scale(1.5);
       }
    }

  .arrow-left-customiser.slick-disabled{
     display:none !important;
  }

  .arrow-right-customiser{
      padding: 0 10px;
      top: 0;
      height: 100%;
      color: #000;
      position: absolute;
      right: 0;
      z-index: 1;
    
    &:focus {
      outline:none;
    }
    [class^="icon-"]:before,
	[class*=" icon-"]:before {
      font-weight: 700;
      opacity: 0.5;
	  transform: scale(1.5);
    }
  }

  .arrow-right-customiser.slick-disabled{
     display:none !important;
  }

}

@media (min-width:799px) {
  .arrow-left-customiser{
    display:none !important;
  }
  .arrow-left-customiser.slick-disabled{
     display:none !important;
  }
  .arrow-right-customiser{
     display:none !important;
  }
  .arrow-right-customiser.slick-disabled{
     display:none !important;
  }
}



