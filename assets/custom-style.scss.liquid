/*********************BEST COLLECTION BLOCKS CSS********************\
\*******************************************************************/

.no-margin{
  margin:0px;
}

.p-0{
  padding:0px !important;
}

.p-b-0{
  padding-bottom:0px !important;
}

.p-t-0{
  padding-top:0px !important;
}

.custom-product-list.product-list{ 
  .product_iamge_overlay{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    overflow: hidden;
    transition: opacity 0.3s ease-in;
  }
  
  .product-details{
    .title {
      font-weight: 700;
      font-size:18px;
      letter-spacing: 1.1px;
      line-height: 22px;
    }
    
    .price{
      font-size:14px;
      letter-spacing: 0.8px;
      line-height: 17px;
    }
  }

  .thumbnail-overlay {
    background:transparent;
    bottom:-10px;
    .info{
      text-align: left;
      top: unset;
      padding: 15px 15px 15px 15px;
      right: unset;
      display: block;
      color: #000;      
    }
    
    .price,
    .title {
      color:#000;
      padding:0px;
      font-family: sofia-pro, sans-serif;
    }
    
    .was_price {
      position:relative;
        span.money:after {
          border-bottom: 0.1em solid #000;
          content: "";
          left: 0;
/*           margin-top: calc(0.1em / 2 * -1); */
          position: absolute;
          right: 0;
          top: 50%;
       }
    }
    
    .title {
         
      @media (max-width:768px) {
          font-weight: 400;
          font-size: 18px;
          letter-spacing: 0.9px;
          line-height: 18px;
          margin-bottom:0px;
      } 
      
      @media (min-width:769px) {
        font-weight: 400;
        font-size:18px;
        letter-spacing: 1.1px;
        line-height: 22px;
        margin-bottom:0px;
      }
      
    }
    
    .price{
       @media (min-width:769px) {
          font-weight: 400;
          font-size:14px;
          letter-spacing: 0.8px;
          line-height: 17px;
          width: 50%;
       }
      
       @media (max-width:768px) {
          font-weight: 400;
          font-size: 12px;
          letter-spacing: 0.6px;
 	      line-height: 13px;
          width: 50%;
      }
    }
    
    .swatch-block {
      width: 50%;
      line-height: 0;
      display: flex;
      justify-content: flex-end;
      z-index: 9999999;
    }
  }
  
  .thumbnail .price span.money {
    color:#000;
    font-weight: 500;
    font-size:14px;
    opacity:0.5;
    
     @media (max-width:768px) {
        font-size:12px;
     }
  }
  
  .thumbnail-overlay .sold-out,
  .thumbnail-overlay .coming-soon{
    color:#000;
  }
  
  //Thumnail Overlay For Mobile
  @media only screen and (max-width: 798px){
    .thumbnail .thumbnail-overlay{
      display: block;
      opacity:1;
      
      .product-details{
         opacity:1;
         transform: unset;
      }
    }
  }
}

