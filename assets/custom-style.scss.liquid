/*********************TWO COLLECTION BLOCKS CSS*********************\
\*******************************************************************/

.two_collection_blocks_wrapper{
  .wrapper_block{
    position:relative;
    
    @media (max-width:767px) {
      margin-bottom:20px;
    }
    
     @media(min-width:769px) {
        &:hover {
          .collection_link_wrapper.home-page {
            .subheading{
              display:block;
            }
          }
        }
      }
    
    @media(min-width:769px) {
        .collection_link_wrapper.home-page{
          .subheading{
            display:none;
          }
        }
      }
    
    .collection_link_wrapper{
      position:absolute;
      
      @media(min-width:769px) {
         top:10px;
         left:15px;
       } 
      
       @media(max-width:768px) {
         top:5px;
         left:10px;
       } 
      
      .action_button{
        color: #fff;
        border-width:0px 0px 0px 0px;
        text-align:left;
        
        @media(min-width:769px) {
          font-size:26px;
        }      
        
        @media(max-width:768px) {
          font-size: 18px;
          letter-spacing: 0;
          line-height: 18px;
        }
      }
      
      
      .subheading{
       .action_button.subtext {
          text-align:left;
          border-width: 0px 0px 1px 0px;
          border-color: #fff;
         
         @media(min-width:769px) {
             font-size: 16px;
          }
          @media(max-width:768px) {
             font-size: 12px;
             display: inline-block;
          }

        }
      }

    }
  }
}

